    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtzdev/object-builder"
  },
  "devDependencies": {
    "fs-extra": "^10.0.0"
  },
  "dependencies": {}
}
