{
  "name": "object-builder",
  "version": "0.5.5",
  "description": "High-Performance ObjectBuilder with DirectX acceleration and aggressive optimizations",
  "main": "scripts/build-classic.js",
  "scripts": {
    "build": "node scripts/build-classic.js",
    "package": ".\\package-simple.bat",
